{
  "name": "@bonhomie/cloudinary-super-uploader",
  "version": "2.0.0",
  "type": "module",
  "description": "A powerful React + Node Cloudinary toolkit with drag & drop, browser compression, EXIF checks, duplicate detection, signed uploads, and more.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./node": {
      "import": "./dist/node/index.js",
      "require": "./dist/node/index.cjs"
    }
  },
  "keywords": [
    "cloudinary",
    "cloudinary upload",
    "react uploader",
    "image uploader",
    "drag and drop",
    "sharp",
    "compress",
    "exif",
    "hash",
    "react component",
    "node cloudinary"
  ],
  "scripts": {
    "build": "tsup src/index.js src/node/index.js --format esm,cjs --minify",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests yet\""
  },
  "author": "Bonhomie",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "dependencies": {
    "cloudinary": "^2.8.0",
    "exif-parser": "^0.1.12",
    "image-hash": "^7.0.1",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
